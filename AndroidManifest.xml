<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.app.swcharsheet"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:targetSdkVersion="18" android:minSdkVersion="7"></uses-sdk>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:allowBackup="true">
        <activity android:name=".SWCharSheet"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".Biocreation">
            <intent-filter>
                <action android:name="android.intent.action.BIOCREATION" />
            </intent-filter>
        </activity>
        <activity android:name=".DBManager">
            <intent-filter>
                <action android:name="android.intent.action.DBMANAGER" />
            </intent-filter>
        </activity>
		<activity android:name=".LoadCharSheet">
            <intent-filter>
                <action android:name="android.intent.action.LOADCHARSHEET" />
            </intent-filter>
        </activity>
        <activity android:name=".DispCharSheet">
            <intent-filter>
                <action android:name="android.intent.action.DISPCHARSHEET" />
            </intent-filter>
        </activity>
    </application>
</manifest> 